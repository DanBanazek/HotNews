@model IEnumerable<BestCaseHotNews.Models.Post>

@{
    ViewBag.Title = "Home Page";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>

            </div>
    </section>
    
}
<h2>Recent News</h2>
<p> @Html.ActionLink("Add Post", "../News/Create")</p>

@Html.Partial("~/Views/Shared/_Posts.cshtml", Model)